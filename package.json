  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EKI-INDRADI/eki-latihan-nodejs-unit-test-typescript/issues"
  },
  "homepage": "https://github.com/EKI-INDRADI/eki-latihan-nodejs-unit-test-typescript#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.4.0",
